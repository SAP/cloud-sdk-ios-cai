{
	"collections": [
	  {
		"collectionName": "cloud-sdk-ios-cai",
		"folders": [
		  {
			"startingFolderPath": "Sources/**/en.lproj/",
			"sourceFilters": ["*.strings"],
			"targetFolderPath": "../[langCode].lproj/"
		  }
		]
	  }
	],
	"defaultConfiguration": {
	  "xlfFolderPath": "/translation/XLF/[fullPath]",
	  "pseudoLocFolderPath": "/translation/2Q/[fullPath]",
	  "oneQFolderPath": "/translation/1Q/[fullPath]",
	  "targetFileNamingConvention": "[fileName].[extension]"
	}
}
